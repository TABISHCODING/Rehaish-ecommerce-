  "components": {
    "schemas": {
      "AddWishlistItemDto": {
        "required": [
          "productId"
        ],
        "type": "object",
        "properties": {
          "productId": {
            "type": "integer",
            "format": "int32"
          },
          "quantity": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "ChangePasswordDto": {
        "type": "object",
        "properties": {
          "currentPassword": {
            "type": "string",
            "nullable": true
          },
          "newPassword": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateAddressDto": {
        "required": [
          "addressLine1",
          "city",
          "country",
          "name",
          "phone",
          "postalCode",
          "state"
        ],
        "type": "object",
        "properties": {
          "name": {
            "minLength": 3,
            "type": "string"
          },
          "addressLine1": {
            "minLength": 1,
            "type": "string"
          },
          "addressLine2": {
            "type": "string",
            "nullable": true
          },
          "city": {
            "minLength": 1,
            "type": "string"
          },
          "state": {
            "minLength": 1,
            "type": "string"
          },
          "postalCode": {
            "minLength": 1,
            "pattern": "^\\d{6}$",
            "type": "string"
          },
          "country": {
            "minLength": 1,
            "type": "string"
          },
          "phone": {
            "minLength": 1,
